"xbindkeys_show"
  control+shift + q

"gmrun"
Alt +F2

#####################################
#### these mappings are for x240 ####
#####################################

# func + f1 - Toggle mute
"amixer set Master toggle"
  m:0x0 + c:121
  XF86AudioMute

# func + f2 - decrease master volume
"amixer sset Master 650-"
  m:0x0 + c:122
  XF86AudioLowerVolume

# func + f3 - increase master volume
"amixer sset Master 650+"
  m:0x0 + c:123
  XF86AudioRaiseVolume

# func + f4
  m:0x0 + c:198
  XF86AudioMicMute

# func + f5 - decrease backlight
"sudo xbacklight -dec 5"
  XF86MonBrightnessDown
  m:0x0 + c:199

# func + f6 - increase backlight
"sudo xbacklight -inc 5"
  XF86MonBrightnessUp
  m:0x0 + c:200

# func + f7
"xscreensaver-command -lock"
  m:0x0 + c:235
  XF86Display

# func + f8
"xscreensaver-command -lock && sudo /usr/sbin/hibernate-ram"
  m:0x0 + c:246
  XF86WLAN

# func + f9
  m:0x0 + c:179
  XF86Tools

# func + f10
  m:0x0 + c:225
  XF86Search

# func + f11
  m:0x0 + c:128
  XF86LaunchA

# func + f12
"xnview ."
  m:0x0 + c:152
  XF86Explorer

# powerbutton on tap hibernate
"xscreensaver-command -lock && sudo /usr/sbin/hibernate"
  m:0x0 + c:124
  XF86PowerOff
