"xbindkeys_show" 
  control+shift + q

"gmrun"
Alt +F2

# decrease backlight
"sudo xbacklight -dec 10"
  XF86MonBrightnessDown
  m:0x0 + c:232

# increase backlight
"sudo xbacklight -dec -10"
  XF86MonBrightnessUp
  m:0x0 + c:233

# decrease master volume
"amixer set Master 5-"
  XF86AudioLowerVolume
  m:0x0 + c:122

# increase master volume
"amixer set Master 5+"
  XF86AudioRaiseVolume
  m:0x0 + c:123

# Toggle mute
"amixer set Master toggle"
  m:0x0 + c:121
  XF86AudioMute
